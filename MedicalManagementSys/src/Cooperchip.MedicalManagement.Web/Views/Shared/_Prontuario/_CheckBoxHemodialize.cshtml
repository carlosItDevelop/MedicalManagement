

<div class="col-md-4">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">Hemodiálise</h4>
        </div>
        <div class="panel-body">
            <div class="form-group">

                <div class="col-lg-12 col-md-12">
                    
                    <ul>
                        <li ng-repeat="hemodialise in hemodialises">
                            <div class="toggle-custom">
                                <label class="toggle" data-on="ON" data-off="OFF">
                                    <input type="radio" ng-model="prontuario.Hemodialise" name="idHemodialise" class="form-control" value="{{hemodialise.Descricao}}" />
                                    <span class="button-radio"></span>
                                </label>
                                <label>{{hemodialise.Descricao}}</label>
                            </div>
                        </li>
                    </ul>

                </div>
                
                <div class="form-group col-lg-12 col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        <input id="HemodialiseData" ng-disabled="getInibirEAnularData(prontuario.Hemodialise, 'Não','HemodialiseData')" 
                               ng-model="prontuario.HemodialiseData" name="HemodialiseData" type="date" class="form-control">
                    </div>
                </div>


            </div>

        </div>
    </div>

</div>