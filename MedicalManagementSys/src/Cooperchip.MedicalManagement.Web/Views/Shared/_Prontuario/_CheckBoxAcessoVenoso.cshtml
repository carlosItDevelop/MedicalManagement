

<div class="col-md-4">
    <!--/  Acesso Venoso  -->
    <div class="panel panel-default">
        <!-- Start .panel -->
        <div class="panel-heading">
            <h4 class="panel-title">Acesso Venoso</h4>
        </div>
        <div class="panel-body">
            <div class="form-group">

                <div class="col-lg-12 col-md-12">

                    <ul>
                        <li ng-repeat="acessovenoso in acessosvenosos">
                            <div class="toggle-custom">
                                <label class="toggle" data-on="ON" data-off="OFF">
                                    <input type="radio"
                                           ng-model="prontuario.AcessoVenoso" 
                                           name="idAcessoVenoso" class="form-control" value="{{acessovenoso.Descricao}}" />
                                    <span class="button-radio"></span>
                                </label>
                                <label>{{acessovenoso.Descricao}}</label>
                            </div>
                        </li>
                    </ul>

                </div>


                <div class="form-group col-lg-12 col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                        <input id="AcessoVenosoData" ng-disabled="getInibirEAnularData(prontuario.AcessoVenoso, 'Sem Acesso', 'AcessoVenosoData')"
                               ng-model="prontuario.AcessoVenosoData"
                               name="AcessoVenosoData" type="date" class="form-control">
                    </div>
                </div>


            </div>

        </div>
    </div>

</div>


