
<div class="col-lg-4 col-md-4 col-sm-4">
    <div class="panel panel-success">
        <div class="panel-heading">
            <h4><b>PROCINÉTICO</b></h4>
        </div>
        <div class="panel-body">
            <div class="form-group">
                <div class="col-lg-12 col-md-12">

                    <ul>
                        <li ng-repeat="procinetico in procineticos">
                            <div class="toggle-custom">
                                <label class="toggle" data-on="ON" data-off="OFF">
                                    <input type="radio" name="procinetica" ng-model="prescricao.Procinetico" value="{{procinetico.Descricao}}" class="form-control" />
                                    <span class="button-radio"></span>
                                </label>
                                <label>{{procinetico.Descricao}}</label>
                            </div>
                        </li>
                    </ul>

                    <hr/>

                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <select class="form-control" name="procineticoinrvalo" id="procineticoinrvalo"
                                ng-disabled="getInibirData(prescricao.Procinetico, 'Não fazer')"
                                ng-model="prescricao.ProcineticoIntervalo"
                                ng-options="procineticoinrvalo.Descricao as procineticoinrvalo.Descricao for procineticoinrvalo in procineticointervalos">
                            <option value="">Selecione o intervalo</option>
                        </select>
                    </div>

                </div>


            </div>

        </div>
    </div>

</div>



