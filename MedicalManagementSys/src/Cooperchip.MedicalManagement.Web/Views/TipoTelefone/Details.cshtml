@model Cooperchip.MedicalManagement.Domain.Entidade.TipoTelefone

@{
    ViewBag.Title = "Detalhes de Tipo de Telefone";
}

@{ Layout = null; }

<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title">MedicalManagement-Sys - @ViewBag.Title</h4>
        </div>
        <div class="modal-body">
            <div class="form-horizontal">

                <div>
                    <h4>Tipo de Telefone</h4>
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Descricao)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Descricao)
                        </dd>

                    </dl>
                </div>

            </div>
        </div>
        <div class="modal-footer">
            <input type="button" value="Cancelar" class="btn btn-default" data-dismiss="modal" />
        </div>

    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->