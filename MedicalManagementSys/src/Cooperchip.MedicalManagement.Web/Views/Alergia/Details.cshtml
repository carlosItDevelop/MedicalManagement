@model Cooperchip.MedicalManagement.Domain.Entidade.Alergia


@{
    ViewBag.Title = "Alergia - Detalhes...";
}

@{ Layout = null;}

<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title">MedicalManagement-Sys - @ViewBag.Title</h4>
        </div>

        <div class="modal-body">
            <div class="form-horizontal">

                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Descricao)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Descricao)
                        </dd>
                    </dl>
                </div>

        </div>
        <div class="modal-footer">
            <input type="button" value="Fechar" class="btn btn-default btn-transparent" data-dismiss="modal" />
        </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
