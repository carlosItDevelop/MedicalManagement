@model IEnumerable<Cooperchip.MedicalManagement.Domain.Entidade.TipoDePrecaucao>

@{
    ViewBag.Title = "Lista de Tipo de Precaução";
}

<h2>Lista de Tipo de Precaução</h2>

<p>
    @Html.ActionLink("Adicionar", "Create", "TipoDePrecaucao", null, new { @class="btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descricao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descricao)
        </td>
        <td>
            @Html.ActionLink("Excluir", "Delete", null, new { id=item.Id }, new {@class="btn btn-danger" })
        </td>
    </tr>
}

</table>
